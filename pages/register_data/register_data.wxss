/* pages/userdata/userdata.wxss */
Page{
    width: 100%;
    height: 100%;
}
.page{
    display:flex;
    justify-content: center;
    align-items: center;
    text-align: center;
}
.bg-image2{
    position: absolute;
    top:0;
    left:0;
    width : 100%;
    height : 30%;
    z-index:-1;
}
.bg-image{
    position: absolute;
    top:0;
    left:0;
    width : 100%;
    height : 100%;
    z-index:-1;
}
.data{
    display:flex;
    height: 100%;
    width: 100%;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}
.flot{
    float: inherit;
  }
  .head{
   margin-top: 20%;
   justify-content: center;
   align-items: center;
   overflow: hidden; 
   border-radius: 80%;
   width: 300rpx;
   height: 300rpx;
   margin: 16rpx 60rpx;
  }
.table{
    display:flex;
    height: 100%;
    width: 100%;
    justify-content: center;
    align-items: center;
    font-size: 30px;
    color: rgb(10, 9, 9);
    flex-direction: column;
    margin-top: 15%;
}
.flex-row{
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-top: 10%;
  }
  .flex-cell{
    flex: 1;

  }
  .check{
    margin-top: 2pc;
    border-radius: 20px;
    display:flex;
    justify-content: center;
    align-items:center;
  }
.icon{
    width: 120rpx;
    height: 120rpx;
    margin-left: 20%;
}